// https://www.codechef.com/problems/THESAV

#include<iostream>
#include<bits/stdc++.h>
using namespace std;

int check(string s){
    stack<char> st;
    int l=s.length();
    for(int i=l-1;i>=0;i--){
        if(s[i]=='*'){
            if(!st.empty())
                st.pop();
            if(!st.empty())
                st.pop();
        }
        else
            st.push(s[i]);
    }
    if(st.empty())
        return 1;
    else
        return 0;
}
int main(){
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        string s;
        cin>>s;
        int ans=check(s);
        if(ans==1)
        cout<<"YES"<<endl;
        else
        cout<<"NO"<<endl;
    }
}
