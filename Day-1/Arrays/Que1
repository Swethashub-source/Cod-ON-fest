/*
https://www.codechef.com/problems/EXPCODE1/
*/

#include <iostream>
#include<bits/stdc++.h>
using namespace std;

void swap(int *x,int *y){
    int t=*x;
    *x=*y;
    *y=t;
}

int partition(int arr[],int low,int high){
    //cout<<"low="<<low<<" high="<<high;
    int pivot=arr[high];
    //cout<<"  pivot="<<pivot<<endl;
    int i=low-1;
    for(int j=low;j<=high-1;j++){
        if(arr[j]<pivot){
            i++;
            swap(&arr[i],&arr[j]);
            //i++;
        }
    }
    swap(&arr[i+1],&arr[high]);
    //cout<<"i="<<i<<endl;
    return i+1;
}

int quickSort(int arr[],int low,int high){
    if (low < high) 
    { 
        int pi = partition(arr, low, high);
        quickSort(arr, low, pi - 1); 
        quickSort(arr, pi + 1, high); 
    } 
}

int main() {
	// your code goes here
	int n;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++)
	cin>>arr[i];
	//int arr[]={11,22,9,44,1};
    int k;
    //int n=5;
    cin>>k;
    k=n-k+1;
    quickSort(arr,0,n-1);
    cout<<arr[n-1]<<endl;
    cout<<arr[k-1];
	return 0;
}
