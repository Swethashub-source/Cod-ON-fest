# https://leetcode.com/problems/find-all-duplicates-in-an-array/

class Solution:
    def findDuplicates(self, nums: List[int]) -> List[int]:
        ans=[]
        l=len(nums)
        for i in range(l):
            idx=abs(nums[i])-1;
            if(nums[idx]<0):
                ans.append(abs(nums[i]))
            else:
                nums[idx]*=-1
        return ans
