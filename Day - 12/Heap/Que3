// https://www.codechef.com/problems/FLOW018

#include <iostream>
using namespace std;
long long int fact(long long int n){
    if(n==0)
        return 1;
    if(n<=2)
        return n;
    return n*fact(n-1);
}

int main()
{
    long long int t,n;
    cin>>t;
    for(int i=0;i<t;i++){
        cin>>n;
        cout<<fact(n)<<endl;
    }
    return 0;
}

