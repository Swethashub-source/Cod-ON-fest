/*
https://www.codechef.com/problems/CHGM1
*/

#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        int n;
        cin>>n;
        int arr[n];
        for(int j=0;j<n;j++)
            cin>>arr[j];
        int max_sum_so_far=INT_MIN,max_ending_here=0;
        for(int k=0;k<n;k++){
            max_ending_here+=arr[k];
            if(max_sum_so_far<max_ending_here)
            max_sum_so_far=max_ending_here;
            if(max_ending_here<0)
            max_ending_here=0;
        }
        cout<<max_sum_so_far<<endl;
    }
    return 0;
}

