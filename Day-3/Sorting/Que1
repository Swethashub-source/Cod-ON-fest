/*
https://practice.geeksforgeeks.org/problems/minimum-swaps/1
*/

// { Driver Code Starts
#include<bits/stdc++.h>
using namespace std;

 // } Driver Code Ends


class Solution 
{
    public:
    //Function to find the minimum number of swaps required to sort the array. 
	int minSwaps(vector<int>&nums)
	{
	    // Code here
	    int n=nums.size();
	    pair<int,int>PosArr[n];
	    for(int i=0;i<n;i++){
	        PosArr[i].first=nums[i];
	        PosArr[i].second=i;
	    }
	    sort(PosArr,PosArr+n);
	    vector<bool> vis(n,false);
	    int minswaps=0;
	    for(int i=0;i<n;i++){
	        if(vis[i] || PosArr[i].second==i) 
	        continue;
	        int j=i;
	        int cyclesize=0;
	        while(!vis[j]){
	            cyclesize++;
	            vis[j]=1;
	            j=PosArr[j].second;
	        }
	        if(cyclesize>0)
	        minswaps+=cyclesize-1;
	    }
	    return minswaps;
	}
};

// { Driver Code Starts.
int main(){
	int tc;
	cin >> tc;
	while(tc--){
		int n;
		cin >> n;
		vector<int>nums(n);
		for(int i = 0; i < n; i++)
			cin >> nums[i];
		Solution obj;
		int ans = obj.minSwaps(nums);
		cout << ans <<"\n";
	}
	return 0;
}  // } Driver Code Ends
