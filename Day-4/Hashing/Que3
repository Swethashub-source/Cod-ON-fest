# https://leetcode.com/problems/longest-consecutive-sequence/

class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        nums=set(nums)
        print(nums)
        n=len(nums)
        l=0
        for num in nums:
            if num-1 not in nums:
                y=num+1
                while(y in nums):
                    y+=1
                l=max(l,y-num)
        return l
        
