https://practice.geeksforgeeks.org/problems/first-and-last-occurrences-of-x3116/1
*/

// { Driver Code Starts
#include<bits/stdc++.h>
using namespace std;
vector<int> find(int a[], int n , int x );

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,x;
        cin>>n>>x;
        int arr[n],i;
        for(i=0;i<n;i++)
        cin>>arr[i];
        vector<int> ans;
        ans=find(arr,n,x);
        cout<<ans[0]<<" "<<ans[1]<<endl;
    }
    return 0;
}


// } Driver Code Ends


vector<int> find(int arr[], int n , int x )
{
    // code here
    int low=0;
    int high=n-1;
    vector<int> ans;
    int firstoccur=-1,lastoccur=-1;
    while(low<=high){
        int mid=(low+high)/2;
        if(arr[mid]==x){
           // cout<<mid<<endl;
            firstoccur=mid;
            high=mid-1;
        }
        else if(arr[mid]>x)
        high=mid-1;
        else low=mid+1;
    }
  // cout<<"firstoccur="<<firstoccur<<endl;
    low=0;
    high=n-1;
    while(low<=high){
        int mid=(low+high)/2;
        if(arr[mid]==x){
           // cout<<mid<<endl;
            lastoccur=mid;
            low=mid+1;
        }
        else if(arr[mid]>x)
        high=mid-1;
        else low=mid+1;
    }
    if(lastoccur==-1 || firstoccur==-1){
        ans.push_back(-1);
        ans.push_back(-1);
    } 
    else{
        ans.push_back(firstoccur);
        ans.push_back(lastoccur);
    }
    return ans;
    
}
